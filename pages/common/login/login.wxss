/* pages/common/login/login.wxss */
.loginBoom{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99;
  background: rgba(0, 0, 0, 0.75)
}
.shadowbox{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display:-moz-box;
  -moz-box-pack:center;
  -moz-box-align:center;
  display:-webkit-box;
  -webkit-box-pack:center;
  -webkit-box-align:center;
  display:box;
  box-pack:center;
  box-align:center;
}
.mainbox{
  width: 80%;
  background: #fff;
  border-radius: 10rpx;
  text-align: center;
  padding: 40rpx 0;
  position: relative;
}
.txt{
  font-size: 32rpx;
  color: #999;
  padding: 0 15rpx 50rpx;
  text-align: center;
  box-sizing: border-box;
}
.txt1{
  font-size: 38rpx;
  padding: 50rpx 0;
  text-align: center;
  color: #000;
}
.btnbox{
  display: box;
  display: -webkit-box;
  border-top: 1px solid #e0e0e0;
}
.btn_child{
  background: none;
  display: inline-block;
  border: 0;
  /* box-flex:1;
  -webkit-box-flex: 1; */
  width: 80%;
  font-size: 32rpx;
  text-align: center;
  padding: 20rpx 0;
  color: #333;
  /* border-top: 1px solid #e0e0e0; */
}
.sure{
  background: #28ad05 !important;
  color: #fff !important;
  border-radius: 12rpx;
  line-height: 40rpx !important;
}
.boomimg{
  display: inline-block;
  width: 600rpx;
  height: 821rpx;
}
.mainbox1{
  position: relative;
  text-align: center;
}
.getbtn{
  display: block;
  width: 100%;
  height: 168rpx;
  position: absolute;
  bottom: 0;
  left: 0;
}
.closebtnpost{
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 40rpx 40rpx;
  background-position: 50% 50%;
  position: absolute;
  right: 0;
  top: 0;
}
.wecat{
  padding: 40rpx 0 40rpx 100rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 80rpx 80rpx;
  background-position: 0 50%;
  display: inline-block;
}