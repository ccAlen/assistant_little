/* pages/inputDirection/inputDirection.wxss */
@import "../inputMajor/inputMajor.wxss";
.loginBoom{
  position:fixed;
  top:0;
  left:0;
  right:0;
  bottom:0;
}
.cancalbtn{
  background:none;
  display:block;
  border-right:1px solid #e0e0e0;
  box-flex:1;
  -webkit-box-flex:1;
  width:50%;
  font-size:32rpx;
  text-align:center;
  padding:20rpx 0;
  color:#333;
  line-height:80rpx;
}
.sharebtnbox{
  border-top:1px solid #e0e0e0;
  display: flex;
}
.txt1{
  padding: 60rpx 0;
}
.adT1{
  font-size: 30rpx;
  color: #333;
  line-height: 50rpx;
  text-align: center;
}
.profbox{
  display: -webkit-flex; 
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
.profchild{
  width: 46%;
  text-align: center;
  background: #eeeded;
  color: #444;
  margin: 4px;
  padding: 30rpx 10rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  border: 1px solid #cccccc;
  border-radius: 12rpx;
}
.refreshbox{
  text-align: center;
  padding: 40rpx 0;
}
.refreshchild{
  display: inline-block;
  padding: 110rpx 20rpx 20rpx 20rpx;
  font-size: 30rpx;
  color: #ccc;
  text-align: center;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100rpx 100rpx;
  background-position: 50% 0;
}
.adtbox{
  padding: 50rpx 0 40rpx;
}
.profchilded{
  background: #fff;
  color: #ffc000;
  border: 1px solid #ffc000;
}
.shadowbox{
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 6;
}
.shareimg{
  display: inline-block;
  width: 600rpx;
  height: 691rpx;
}
.sharebox{
  position: relative;
  z-index: 7;
  width:600rpx;
  height:691rpx;
}
.boomcenter{
  position:fixed;
  top:0;
  left:0;
  right:0;
  bottom:0;
  display: -webkit-flex; 
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  z-index: 7;
  align-items:center;
}
.cancal{
  display: inline-block;
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  top: 0;
  right: 0;
}
.sharebtn{
  display: block;
  width: 100%;
  height: 140rpx;
  position: absolute;
  bottom: 20rpx;
  left: 0;
}