  top: -450rpx;
  left: -8rpx;
  background: #ffe9a7;
  border: 1px solid #f4c964;
  border-radius: 50%;
  box-sizing: border-box;
}
.centerbox{
  padding: 0 5%;
  box-sizing: border-box;
}
.headerbox{
  border-bottom: 1px solid #eee;
  padding: 148rpx 0 40rpx;
  text-align: center;
  z-index:8;
  position:relative;
}
.headericon{
  display: inline-block;
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  border: 2px solid #fff;
}
