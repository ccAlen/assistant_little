/* pages/textualResearch/textualResearch.wxss */ 
.nothing{
  font-size: 34rpx;
  color: #666;
  padding: 50rpx 5%;
  box-sizing: border-box;
  height: 948rpx;
}
.recordNav {
padding:28rpx 8%;
display:box;
display:-webkit-box;
box-sizing: border-box;
}
.nav_child {
  box-flex:1;
-webkit-box-flex:1;
font-size:30rpx;
color:#666;
padding:0 40rpx 0 0;
text-align:center;
}
.nav_child {
  flex:1;
}
.nav_selected {
color:#ffc000;
font-size: 36rpx;
font-weight: bold;
}
.rcnavbox{
  position: relative;
  text-align: center;
  /* padding-bottom: 15rpx; */
  border-bottom: 1px solid #e5e5e5;

}
.searchicon{
  display: inline-block;
  width: 20%;
  height: 100rpx;
  position: absolute;
  left: 0;
  top: 0;
  background: url("data:image/png;base64,") no-repeat;
  background-size:42rpx 42rpx;
background-position:50% 40%;
}
.trnav{
  padding: 0 5%;
  box-sizing: border-box;
}
.searchbox{
  width: 100%;
  /* border: 1px solid #ddd; */
  border-radius: 12rpx;
  position: relative;
  text-align: center;
  padding: 0 100rpx;
  box-sizing: border-box;
  /* margin-bottom: 30rpx; */
  background: #f5f5f5;
}
.searchback{
  font-size: 30rpx;
  color: #666;
  width: 120rpx;
  text-align: center;
  position: absolute;
  left: 0;
  padding: 26rpx 0;
}
.searchback::before{
  display: inline-block;
  content: '';
  width: 1px;
  height: 80%;
  position: absolute;
  right: 0;
  top: 10%;
  background: #ddd;
}
.searchinp{
  font-size: 30rpx;
  color: #666;
  text-align: center;
  padding: 20rpx 0;
}
.s1{
  width:100rpx;
  height:92rpx;
  position: absolute;
  right: 0;
  top:0;
}
.hot_left{
  padding: 0 0 0 20rpx;
}
.fixedposition{
  position: fixed;
  background: #fff;
  width: 90%;
  top: 0;
  left: 5%;
}
.emptyboxtr{
  display: block;
  width: 100%;
  height: 130rpx;
}