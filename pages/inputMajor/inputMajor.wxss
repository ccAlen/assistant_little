/* pages/inputMajor/inputMajor.wxss */
.img1{
  display: inline-block;
  width: 345rpx;
  height: 420rpx;
}
.majorcontent{
  padding: 40rpx 5%;
  box-sizing: border-box;
}
.inputbox{
  display: flex;
  width: 100%;
  border: 1px solid #eee;
  position: relative;
  padding-left: 70rpx;
  box-sizing: border-box;
}
.searchicon{
  display: inline-block;
  width: 37rpx;
  height: 38rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
  position: absolute;
  left: 25rpx;
  top: 50%;
  margin-top: -17rpx;
}
.majorinput{
  flex: 2.5;
  display: block;
  width: 70%;
  padding: 0 20rpx;
  box-sizing: border-box;
  font-size: 32rpx;
  color: #666;
  border-right: 1px solid #eee;
  height: 95rpx;
  line-height: 95rpx;
}
.nextstap{
  flex: 1;
  width: 30%;
  font-size: 32rpx;
  background: #ffc000;
  text-align: center;
  padding: 25rpx 15rpx;
  box-sizing: border-box;
  color: #fff;
}
.imcenter{
  text-align: center;
  padding: 80rpx 0 55rpx;
}
.imicon{
  display: inline-block;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin-bottom: 30rpx;
}
.imT1{
  font-size: 32rpx;
  color: #ffc000;
  text-align: center;
  padding: 5rpx 0;
}
.imT2{
  font-size: 32rpx;
  color: #999;
  text-align: left;
  padding: 10rpx 0 20rpx;
  line-height: 65rpx;
}
.searchcontent{
  position: relative;
}
.searchresult{
  position: absolute;
  top: 100rpx;
  left: 0;
  background: #fff;
  width: 100%;
  box-shadow:0 3px 10px -3px #999;
  max-height: 915rpx;
  overflow-y: scroll;
  padding: 0 4%;
  box-sizing: border-box;
  -webkit-overflow-scrolling: touch;
}
.srList{
  font-size: 30rpx;
  color: #666;
  padding: 15rpx 0;
}
.srTop{
  text-align: center;
  font-size: 32rpx;
  padding: 65rpx 0 25rpx;
  position: relative;
  color: #999;
}
.srTop::before{
  display: inline-block;
  content: '';
  position: absolute;
  left: 50%;
  top: 10rpx;
  width: 50rpx;
  height: 50rpx;
  margin-left: -25rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
}
.srListed{
  color: #ffc000;
}
.wrong{
  display: inline-block;
  width: 146rpx;
  height: 125rpx;
}
.wrongbox{
  padding: 55rpx 0;
  text-align: center;
}
.wrongTxt{
  font-size: 26rpx;
  color: #666;
  text-align: center;
  line-height: 50rpx;
}
.suggestLink{
  display: inline-block;
  text-decoration: underline;
  color: #ffc000;
}
.padB50{
  padding-bottom: 50rpx;
}
.imT3{
  font-size: 34rpx;
  font-weight: bold;
  color: #999;
  line-height: 65rpx;
}