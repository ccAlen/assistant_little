/* pages/notes/notes.wxss */ 
page{
  background: #d3f3f3;
}
.linebox{
  width: 90%;
  margin:40rpx 5%;
  height: 2px;
  background: #ffc000;
}
.details{
  /*margin-left: 15rpx;*/
  text-decoration: underline;
  color: #999;
  margin-left: 18rpx;
}
/*.shu{
  padding: 0 15rpx;
}*/
canvas{
  position: relative;
  z-index: -1;
}
.nothing {
  font-size: 32rpx;
  color: #666;
  padding: 30rpx 2%;
  box-sizing: border-box;
}

.notecontent {
  text-align: center;
  padding: 60rpx 0 0;
  box-sizing: border-box;
  z-index: 0;
  position: relative;
}

.notestitle {
  font-size: 34rpx;
  color: #333;
  /* border-bottom: 2px solid #ffc000; */
  /* display: inline-block; */
  padding: 8rpx 0;
}

/* .notestitle text {
  color: #ffc000;
  font-weight: bold;
} */

.canbox {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: -15rpx;
}

.typeList {
  font-size: 32rpx;
  color: #333;
  padding: 10rpx 0;
  display: block;
  /* width: 50%; */
  text-align: left;
  margin-right: 20rpx;
}

.typebox {
  margin-top: -50rpx;
  padding: 0 10%;
  box-sizing: border-box;
  position: relative;
  z-index: 9;
  /* display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
  align-items: left;
  margin-right: 40rpx; */
}

.colorbox {
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  background: pink;
  vertical-align: 4rpx;
  margin-right: 15rpx;
  border-radius: 50%;
}

.notestips {
  font-size: 24rpx;
  color: #999;
  text-align: center;
  padding: 40rpx 0;
}

.bottomNav {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  border-top: 1px solid #ddd;
  display: -webkit-box;
  display: box;
  padding: 12rpx 0 5rpx;
  background: #fff;
  z-index: 8;
}

.bottomNav_child {
  -webkit-box-flex: 1;
  box-flex: 1;
  width: 33%;
  text-align: center;
  position: relative;
}

.c1 {
  background: url("data:image/png;base64,") no-repeat;
}

.c2 {
  background: url("data:image/png;base64,") no-repeat;
}

.icon, .navselected .icon {
  display: inline-block;
  width: 42rpx;
  height: 42rpx;
  background-size: 100%;
}

.navtxt {
  font-size: 20rpx;
  color: #333;
  line-height: 26rpx;
}

.sharebtn {
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  line-height: 122rpx;
}

.posterContent {
  position: fixed;
  bottom: 0;
  left: 0;
  top: 0;
  right: 0;
  z-index: 9;
}

.grayShadow {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.8);
  z-index: 3;
}

.posterMain {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: #fff;
  z-index: 4;
  text-align: center;
}

.posterimg {
  display: inline-block;
  width: 400rpx;
  height: 712rpx;
  border-radius: 25rpx;
}

.pt1 {
  font-size: 30rpx;
  color: #888;
  padding: 20rpx 0;
  text-align: center;
}

.save {
  text-align: center;
  background: #ffc000;
  border-radius: 10rpx;
  padding: 15rpx 60rpx;
  color: #fff;
  font-size: 34rpx;
  display: inline-block;
  width: 30%;
}
.dowloadicon{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  /* vertical-align:middle; */
  margin-right: 10rpx;
}
.st {
  display: inline-block;
  vertical-align:top;
}
.relativebox{
  position: relative;
  padding: 60rpx 8% 40rpx;
  box-sizing: border-box;
}
.closebtnpost{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 4%;
  top: 20rpx;
}
.notebanner{
  display: block;
  width: 750rpx;
  height: 630rpx;
}
.notebox{
  background: #fff;
  width: 94%;
  margin:0 3%;
  box-sizing: border-box;
  border-radius: 15rpx;
  padding: 0 4%;
}
.tyscirl{
  display: inline-block;
  /* width: 68%; */
  text-align: center;
  background: #ffc500;
  border-radius: 100rpx;
  padding: 15rpx 40rpx;
  color: #fff;
}
.tablebox{
  border: 1px solid #e0e0e0;
  width: 100%;
  margin-top:-40rpx;
}
.tdbox{
  display: flex;
  display: -webkit-flex;
  flex-wrap: wrap;
  border-bottom: 1px solid #e0e0e0;
}
.tdbox:last-child{
  border-bottom: 0;
}
.tlbox{
  padding: 25rpx 20rpx;
  box-sizing: border-box;
}
.tl1{
  border-right: 1px solid #e0e0e0;
  font-size: 28rpx;
  color: #333;
  width: 50%;
  text-align: left;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.tl2{
  font-size: 28rpx;
  width: 50%;
}
.compensation{
  display: inline-block;
  font-size: 24rpx;
  color: #ffa200;
  padding-left: 30rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 20rpx 18rpx;
  background-position: 0 50%;
  margin-right: 30rpx;
} 