/**app.wxss**/
/*清除默认样式  */
element {-webkit-overflow-scrolling: touch;}
button{
  margin: 0;
  padding: 0;
  border: 0 !important;
  outline: none;
  list-style-type:none;
  border-radius: 0;
  box-shadow: none;
  background: none;
}
/* 公共样式 */
.borderTop{
  border-top: 1px solid #e3e1ec;
}
.gzhshouquan{
  position:fixed;
  bottom:6rpx;
  width:99%;
  height:168rpx;
  
  margin-left: 0.2%;
  /* box-sizing: border-box; */


  /* border-radius: 25rpx; */
}
.accountsy,.rebox{
  position: relative;
  width:100%;
  height:168rpx;
}
.accountsy{
  border: 1px solid #1aad19;
}
.gzhclose{
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 30rpx 30rpx;
  background-position: 50% 10rpx;
  right: 4%;
  top: 0;
  z-index: 99;
}
/* 首页列表公共样式 */
image,video{
  margin: 0;
}
.bgwhite{
  background: #fff;
}
.padlf{
  padding: 0 5%;
  box-sizing: border-box;
}
.magb{
  margin-bottom: 18rpx;
}
.emptybox{
  height: 100rpx;
}
.emptybox12{
  height: 120rpx;
}
.grayemptybox{
  height: 18rpx;
  background: #e8e8e8;
  width: 100%;
}
.module{
  background: #fff;
  padding: 20rpx 5%;
  box-sizing: border-box;
  margin-bottom: 18rpx;
}
.module1{
  background: #fff;
  padding: 20rpx 5%;
  box-sizing: border-box;
}
.module2{
  background: #fff;
  padding: 40rpx 5%;
  box-sizing: border-box;
  margin-bottom: 18rpx;
  text-align: center;
}
.module_title{
  padding: 15rpx 100rpx 15rpx 0;
  position: relative;
  font-size: 36rpx;
  font-weight: bold;
  color: #000;
}
.recommend_module{
  display: flex;
  /* align-items: center; */
}
.hot_module,.recommend_module{
  display: flex;
  border-bottom: 1px solid #e5e5e5;
  padding: 25rpx 0;
}
.hot_module:last-child,.recommend_module:last-child{
  border-bottom: 0;
}
.hot_left{
  flex: 1.8;
  padding-right: 20rpx;
  box-sizing: border-box;
}
.hot_right{
  flex: 1;
  border-radius: 15rpx;
  overflow: hidden;
  max-height: 190rpx;
}
.hotimg{
  display: block;
  width: 254rpx;
  height: 192rpx;
  border-radius:15rpx;
}
.hlT1{
  font-size: 30rpx;
  color: #000;
  line-height: 46rpx;
}
.hlT2{
  font-size: 24rpx;
  color: #999;
  line-height: 35rpx;
  padding-top: 15rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -o-text-overflow:ellipsis;
  -white-space: nowrap;
  display: -webkit-box;
  -webkit-line-clamp: 4;
  line-clamp:4;
  -webkit-box-orient: vertical;
  word-break: break-all;
}
.more_icon{
  display: inline-block;
  width: 100rpx;
  height: 67rpx;
  background: url("data:image/png;base64,") no-repeat; 
  background-size: 29rpx 24rpx;
  background-position: 80% 50%;
  position: absolute;
  top: 0;
  right: 0;
}
.recommend_left{
  flex: 1;
  border-radius: 15rpx;
  overflow: hidden;
  width: 178rpx;
  height: 178rpx;
  position: relative;
}
.recommend_right{
  flex: 2.6;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.rlT1{
  font-size: 30rpx;
  position: relative;
  padding-right: 120rpx;
}
.rlT2{
  font-size: 26rpx;
  color: #6e6e6e;
  padding: 14rpx 0 14rpx 0;
  width: 470rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.peopleimg{
  display: block;
  width: 178rpx;
  height: 178rpx;
  border-radius: 15rpx;
}
.rlT2 view{
  display: inline-block;
}
.rlT2 .name{
  padding-right: 20rpx;
  margin-right: 0;
}
.xianshi{
  position: absolute;
  left: 0;
  top: 0;
  width: 58rpx;
  height: 55rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
}
.jobMarket{
  display: inline-block;
  padding: 3rpx 12rpx 6rpx;
  font-size: 22rpx;
  color: #55b387;
  border: 1px solid #55b387;
  border-radius: 10rpx;
  position: absolute;
  right: 0;
  top: 5rpx;
  max-width: 112rpx;
}
.countbox{
  text-align: right;
}
.count{
  display: inline-block;
  padding: 0 8rpx 5rpx;
  background: #ffc000;
  color: #fff;
  font-size: 22rpx;
  border-radius: 10rpx;
}
.recommend_bottom{
  position: relative;
  /* padding-right: 160rpx; */
  display: box;
}
.recommend_bottom view{
  display: inline-block;
  box-flex:1;
}
.time{
  color: #999;
  padding-left: 28rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 21rpx 21rpx;
  background-position: 0 50%;
  font-size: 22rpx;
}
.num{
  color: #999;
  padding-left: 25rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 16rpx 20rpx;
  background-position: 0 50%;
  margin-left: 30rpx;
  font-size: 22rpx;
  margin-right: 30rpx;
}
.price{
  color: #ffa537;
  font-size: 26rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}
.grayprice{
  color: #999;
  text-decoration: line-through;
  margin-right: 14rpx;
}
/* 修改编辑器默认样式 */
.htmltowxml_content{
  padding: 0 5%;
  box-sizing: border-box;
}
.html2wxml--html2wxml{
  padding: 0 !important;
}
/* pages/common/login/login.wxss */
.getphoneBoom{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 100;
  background: rgba(0, 0, 0, 0.5)
}
.shadowbox{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display:-moz-box;
  -moz-box-pack:center;
  -moz-box-align:center;
  display:-webkit-box;
  -webkit-box-pack:center;
  -webkit-box-align:center;
  display:box;
  box-pack:center;
  box-align:center;
}
.mainbox{
  width: 70%;
  background: #fff;
  border-radius: 10rpx;
  text-align: center;
}
.txt{
  font-size: 32rpx;
  color: #999;
  padding: 0 15rpx 50rpx;
  text-align: center;
  box-sizing: border-box;
}
.txt1{
  font-size: 38rpx;
  padding: 50rpx 0 20rpx;
  text-align: center;
  color: #000;
}
.btnbox{
  display: box;
  display: -webkit-box;
  /* border-top: 1px solid #e0e0e0; */
}
.btn_child{
  background: none;
  display: block;
  border: 0;
  box-flex:1;
  -webkit-box-flex: 1;
  width: 50%;
  font-size: 32rpx;
  text-align: center;
  padding: 20rpx 0;
  color: #333;
  border-top: 1px solid #e0e0e0;
}
.sure{
  color: #ffc000 !important;
  /* border-left: 1px solid #e0e0e0 !important; */
}
.buytipstxt{
  font-size: 34rpx;
  color: #333;
  padding: 60rpx 15rpx;
  text-align: center;
  box-sizing: border-box;
}
